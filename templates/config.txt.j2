################################################################################
##  Standard Definition Video Settings
################################################################################

{% if raspberrypi__sdtv_mode is defined %}
## sdtv_mode
##     defines the TV standard for composite output
##
##     Value    Description
##     -------------------------------------------------------------------------
##     0        Normal NTSC (Default)
##     1        Japanese version of NTSC - no pedestal
##     2        Normal PAL (European)
##     3        Brazilian version of PAL - 525/60 rather than 625/50, different
##              subcarrier
##
sdtv_mode={{ raspberrypi__sdtv_mode|int }}
{% endif %}

{% if raspberrypi__sdtv_aspect is defined %}
## sdtv_aspect
##     defines the aspect ratio for composite output
##
##     Value    Description
##     -------------------------------------------------------------------------
##     1        4:3 (Default)
##     2        14:9
##     3        16:9
##
sdtv_aspect={{ raspberrypi__sdtv_aspect|int }}
{% endif %}

{% if raspberrypi__sdtv_disable_colourburst is defined %}
## sdtv_disable_colourburst
##     Disables colour burst on composite output. The picture will be
##     monochrome, but possibly sharper
##
##     Value    Description
##     -------------------------------------------------------------------------
##     0        Colour burst is enabled (Default)
##     1        Colour burst is disabled
##
sdtv_disable_colourburst={{ raspberrypi__sdtv_disable_colourburst|int }}
{% endif %}


################################################################################
##  High Definition Video Settings
################################################################################

{% if raspberrypi__hdmi_safe is defined %}
## hdmi_safe
##     Use "safe mode" settings to try to boot with maximum hdmi compatibility.
##
##     Value    Description
##     -------------------------------------------------------------------------
##     0        Disabled (Default)
##     1        Enabled (this does: hdmi_force_hotplug=1,
##                                  hdmi_ignore_edid=0xa5000080,
##                                  config_hdmi_boost=4, hdmi_group=2,
##                                  hdmi_mode=4, disable_overscan=0,
##                                  overscan_left=24, overscan_right=24,
##                                  overscan_top=24, overscan_bottom=24)
##
hdmi_safe={{ raspberrypi__hdmi_safe|int }}
{% endif %}

{% if raspberrypi__hdmi_force_hotplug is defined %}
## hdmi_force_hotplug
##     Pretends HDMI hotplug signal is asserted so it appears a HDMI display
##     is attached
##
##     Value    Description
##     -------------------------------------------------------------------------
##     0        Disabled (Default)
##     1        Use HDMI mode even if no HDMI monitor is detected
##
hdmi_force_hotplug={{ raspberrypi__hdmi_force_hotplug|int }}
{% endif %}

{% if raspberrypi__hdmi_ignore_hotplug is defined %}
## hdmi_ignore_hotplug
##     Pretends HDMI hotplug signal is not asserted so it appears a HDMI
##     display is not attached
##
##     Value    Description
##     -------------------------------------------------------------------------
##     0        Disabled (Default)
##     1        Use composite mode even if HDMI monitor is detected
##
hdmi_ignore_hotplug={{ raspberrypi__hdmi_ignore_hotplug|int }}
{% endif %}

{% if raspberrypi__hdmi_drive is defined %}
## hdmi_drive
##     chooses between HDMI and DVI modes
##
##     Value    Description
##     -------------------------------------------------------------------------
##     1        Normal DVI mode (No sound)
##     2        Normal HDMI mode (Sound will be sent if supported and enabled)
##
hdmi_drive={{ raspberrypi__hdmi_drive|int }}
{% endif %}

{% if hdmi_ignore_edid is defined %}
## hdmi_ignore_edid
##     Enables the ignoring of EDID/display data
##
hdmi_ignore_edid={{ hdmi_ignore_edid }}
{% endif %}

{% if raspberrypi__hdmi_edid_file is defined %}
## hdmi_edid_file
##     Read the EDID data from the edid.dat file instead of from the attached
##     device
##
##     Value    Description
##     -------------------------------------------------------------------------
##     0        Read EDID data from attached device (Default)
##     1        Read EDID data from edid.txt file
##
hdmi_edid_file={{ raspberrypi__hdmi_edid_file|int }}
{% endif %}

{% if raspberrypi__hdmi_ignore_edid_audio is defined %}
## hdmi_ignore_edid_audio
##     Pretends all audio formats are unsupported by display. This means ALSA
##     will default to analogue.
##
##     Value    Description
##     -------------------------------------------------------------------------
##     0        Use EDID provided values (Default)
##     1        Pretend all audio formats are unsupported
##
hdmi_ignore_edid_audio={{ raspberrypi__hdmi_ignore_edid_audio|int }}
{% endif %}

{% if raspberrypi__hdmi_force_edid_audio is defined %}
## hdmi_force_edid_audio
##     Pretends all audio formats are supported by display, allowing
##     passthrough of DTS/AC3 even when not reported as supported.
##
##     Value    Description
##     -------------------------------------------------------------------------
##     0        Use EDID provided values (Default)
##     1        Pretend all audio formats are supported
##
hdmi_force_edid_audio={{ raspberrypi__hdmi_force_edid_audio|int }}
{% endif %}

{% if raspberrypi__hdmi_force_edid_3d is defined %}
## hdmi_force_edid_3d
##     Pretends all CEA modes support 3D even when edid doesn't indicate
##     support for them.
##
##     Value    Description
##     -------------------------------------------------------------------------
##     0        Use EDID provided values (Default)
##     1        Pretend 3D mode is supported
##
hdmi_force_edid_3d={{ raspberrypi__hdmi_force_edid_3d|int }}
{% endif %}

{% if raspberrypi__avoid_edid_fuzzy_match is defined %}
## avoid_edid_fuzzy_match
##     Avoid fuzzy matching of modes described in edid.
##
##     Value    Description
##     -------------------------------------------------------------------------
##     0        Use fuzzy matching (Default)
##     1        Avoid fuzzy matching
##
avoid_edid_fuzzy_match={{ raspberrypi__avoid_edid_fuzzy_match|int }}
{% endif %}

{% if raspberrypi__hdmi_pixel_encoding is defined %}
## hdmi_pixel_encoding
##     Force the pixel encoding mode.
##     By default it will use the mode requested from edid so shouldn't
##     need changing.
##
##     Value    Description
##     -------------------------------------------------------------------------
##     0        Use EDID provided values (Default)
##     1        RGB limited   (16-235)
##     2        RGB full      ( 0-255)
##     3        YCbCr limited (16-235)
##     4        YCbCr limited ( 0-255)
##
hdmi_pixel_encoding={{ raspberrypi__hdmi_pixel_encoding|int }}
{% endif %}

{% if raspberrypi__hdmi_group is defined %}
## hdmi_group
##     Defines the HDMI type
##
##     Value    Description
##     -------------------------------------------------------------------------
##     0        Use the preferred group reported by the edid (Default)
##     1        CEA
##     2        DMT
##
hdmi_group={{ raspberrypi__hdmi_group|int }}
{% endif %}

{% if raspberrypi__hdmi_mode is defined %}
